{
    "name":"FocusFLow",
    "version":"1.0",
    "description":"A program that helps you stay focused VIA the Pomodoro Technique",
    "manifest_version":3,
    "author":"Vinayak Kapoor and Ali Naqvi",
    "permissions": ["storage"],
    "action":{
        "default_popup":"index.html",
        "default_width": 400,
        "default_height": 600},
    "background.service_worker": {
        "script": ["script.js"],
        "persistent": true
      }
    
}